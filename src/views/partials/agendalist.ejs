<% agendalist.forEach(function(agenda) { %>
    <% if (agenda.type == 'directory') { %> 
        <li class="nav-item">
            <a href="?agenda=<%= agenda.name %>&file=<%= agenda.children[0].name %>" class="nav-link<%- currentagenda == agenda.name ? ' active' : null %>" >
                <i class="fa fa-circle-o nav-icon"></i>
                <p><%= agenda.name %></p>
            </a>
        </li>
    <% } %>
<% }); %>