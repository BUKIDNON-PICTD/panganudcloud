<% clientlist.forEach(function(client) { %>
    <% if (client.find(i => i.name === 'clienttype').value == 'location') { %> 
        <li class="nav-item">
            <a href="?clientid=<%= client.find(i => i.name === 'clientid').value %>" class="nav-link" >
                <i class="fa fa-circle-o nav-icon"></i>
                <p><%= client.find(i => i.name === 'clientname').value %></p>
            </a>
        </li>
    <% } %>
<% }); %>